{
    "name": "grpaiva/jetstream-flux",
    "description": "A Laravel package to integrate Jetstream views with Flux UI.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.2",
        "livewire/livewire": "^3.0",
        "livewire/flux": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Grpaiva\\JetstreamFlux\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grpaiva\\JetstreamFlux\\JetstreamFluxServiceProvider"
            ]
        }
    }
}
